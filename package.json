    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.3",
    "express-session": "^1.17.0",
    "randanimal": "^1.0.4",
    "rpc-websockets": "^5.0.8",
    "slugify": "^1.4.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
